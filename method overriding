// method overriding : 

class Calc
{
  public int add(int n1, int n2){
    return n1+n2;
  }
}

class AdvCalc extends Calc
{
    public int add(int n1, int n2)
    {
        return n1+n2+1; // same method name and same parameters but different work


}
}
public  class main
{
    public static void main(String args[])
    {
     AdvCalc obj = new AdvCalc();
     int r1= obj.add(2, 5);
     System.out.println(r1);
    }
}
