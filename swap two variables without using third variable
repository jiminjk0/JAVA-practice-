public class Swap
{

    public static void main(String[] args)
    {
        // not using third variable
        int a = 10;
        int b = 20;

        a = a+b; // 10+20 = 30
        b=a-b; //30-20 = 10
        a= a-b;// 20-10 = 20

        System.out.println("a is " + a + " and" + " " + "b is " + b);


    }
}
