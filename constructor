//constructors  : default and parameterized

 class Human{
    private int age;
    private String name;

    // default constructor
    public Human(){
        System.out.println("this is a constructor");
        age = 22;
        name = "john";
    }
     

    //parameterized

    public Human(int age, String name){
        this.age = age;
        this.name = name;
    }


 public void setAge(int j){
     age = j;
 }
 public void setName(String temp){
     name = temp;
}

 public int getAge(){
    return age;
}
public String getName(){
    return name;
 }

 }

public  class main
 {
    public static void main(String args[])
    {
        Human obj = new Human(); // everytime we create an object it will call the constructor
        Human obj1 = new Human();
        Human obj2 = new Human(10, "amrita");

      obj1.setAge(20);
      obj1.setName("rajanya");

        System.out.println(obj.getAge() + " " + obj.getName());
        System.out.println(obj1.getAge() + " " + obj1.getName());
        System.out.println(obj2.getAge() + " " + obj2.getName());

    }
}

 
