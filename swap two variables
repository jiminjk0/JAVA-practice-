public class Swap
{

    public static void main(String[] args)
    {
        int a = 10;
        int b=20;
        System.out.println("before swapping the values are" + " " + a +  " " + b);
        
        int temp = a;
        a=b;
        b=temp;
        System.out.println("value of a is" + " " + a +" and value of b is" + " " + b);
    }
}
